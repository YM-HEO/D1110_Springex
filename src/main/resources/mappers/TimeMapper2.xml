<?xml version="1.0" encoding="UTF-8" ?>
<!-- XML로 SQL분리방법 2.XML 파일 SQL 작성 후 root-context.xml 이동 -->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  <!-- dtd: Mapper 생성 표준 규격 -->
<mapper namespace="net.ict.d1110_springex.mapper.TimeMapper2"> <!-- nameSpace: 공간(위치) -->
    <select id="getNow" resultType="String">
        select now()
    </select>
</mapper>